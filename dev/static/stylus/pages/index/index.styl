.color-choice
  display: none;

.color-pick
  border-radius: 50px
  display: flex
  margin-right: 5px
  width: 28px
  height: 28px
  background: white
  border: 1px solid #edece9
  position: relative;
  cursor: pointer;
  &:after
    content: ''
    position: absolute;
    border-radius: 50px
    display: flex
    width: 15px
    height: 15px
    background: black
    top 50%
    left: 50%
    transform: translate(-50%,-50%)
    transition-animation()
    transition-delay: 0.1s

.color-choice
  &:checked + .color-pick
    border: 1px solid skyblue
    background: skyblue
    transition-animation()
    transition-delay: 0.1s

.pink
