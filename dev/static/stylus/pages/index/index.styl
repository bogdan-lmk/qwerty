.item
  position: relative;
  width: 310px
  height: 50%
  max-width: 310px
  display: flex;
  flex-direction: column;
  padding: 0 20px
  border: 1px solid #e3e3e3
  border-collapse: collapse
  transition-animation()
  &:hover
    border: 1px solid skyblue
    transition-animation()
    cursor: pointer;
    border-collapse: collapse
    border-bottom: 1px solid transparent
  &:hover .item-action
    display: flex;
    animation: fadeEffect 0.5s
    z-index: 99
  &:hover .item-link-view
    opacity: 1
    transition-animation()

.item-explore
  border: 1px solid #e3e3e3
  align-items: center;
  justify-content: center;
  position: relative
  &:before
    height: 100%
    width: 100%
    content: ''
    position: absolute;
    left 0
    bottom: 0
    background-image: url('/static/img/general/explore-item-decor.png')
    background-repeat: no-repeat;
  &:hover
    border: 1px solid skyblue
    transition-animation()
    background: #81d7ff
  &:hover .explore-collection-link
    color: white
    transition-animation()
  &:hover:before
    background-image: url('/static/img/general/explore-item-decor-blue.png')
    background-repeat: no-repeat;
    transition-animation()

@keyframes fadeEffect{
  from {opacity: 0;}
  to {opacity: 1;}
}



